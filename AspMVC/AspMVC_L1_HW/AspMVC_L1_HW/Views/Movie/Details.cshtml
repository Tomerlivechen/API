@model Movie

<div>
    <h1>Movie Details</h1>
    <h1><b>@Model.Title</b></h1>
    <p><strong>Description:</strong> @Model.Description</p>
    <p><strong>Overview:</strong> @Model.Overview</p>
    <p> <img src="@Model.GetBigImage()" /></p>

    <button class="btn btn-info" onclick="window.location.href='/Movie/';">Back</button>
</div>